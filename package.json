{
  "name": "Sandbox QuickJS and Deno",
  "description": "An example app created with Deno",
  "type": "module",
  "scripts": {
    "dev": "deno run --watch --deny-env --allow-env=PORT,HOST --allow-read=. --allow-net=127.0.0.1:8081 --unstable-worker-options --no-prompt main.js",
    "dev-docker": "docker run eu.gcr.io/preparsed/sandbox-deno:latest"
  },
  "dependencies": {
    "quickjs-emscripten": "^0.29.0"
  }
}
